# AXOL XOR Circuit
# XOR(true, false) = true 
# XOR(a,b) = AND(OR(a,b), NOT(AND(a,b)))

# Step 1: A OR B
gate or { a = [0.1, 0.9], b = [0.9, 0.1] }

# Step 2: A AND B
gate and { a = [0.1, 0.9], b = [0.9, 0.1] }

# Step 3: NOT(A AND B)
gate not { x = [0.9, 0.1] }

# Step 4: AND(OR_result, NAND_result) = XOR
gate and { a = [0.1, 0.9], b = [0.1, 0.9] }
